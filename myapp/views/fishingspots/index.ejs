<h1>釣り場一覧だよ</h1>
<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>アクション</th>
    </tr>
  </thead>
  <tbody>
    <% fishingspots.forEach(fishingspot => { %>
      <tr>
        <td><%= fishingspot.id %></td>
        <td><%= fishingspot.name %></td>
        <td>
          <a href=<%= `/fishingspots/${fishingspot.id}/edit` %>>
            編集
          </a>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>